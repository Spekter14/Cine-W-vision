  <!-- CONTENIDO -->
  <section class="py-5 container1">
   <label>Historial de boletos: <%= name %></label>
     <table class="table table-bordered table-striped text-center mt-4">
        <thead>
          <tr class="bg-primary text-white">
            <th scope="col">ID</th>
            <th scope="col">ID</th>
            <th scope="col">CANTIDAD BOLETOS</th>            
            <th scope="col">TOTAL$</th>      
            <th scope="col">PELICULA</th>
            <th scope="col">HORA</th>                
          </tr>
        </thead>
        <tbody>
          <% results.forEach((results)=>{ %>
            <tr>  
                <td><%= results.id %></td>
                <td><%= results.boleto_id %></td> 
                <td><%= results.boletos%></td>  
                <td><%= results.total%></td>  
                <td><%= results.pelicula%></td>  
                <td><%= results.hora%></td> 
              
            </tr>                                   
         <% }) %>
        </tbody>
      </table>


  </section>
  


